snippet code_is_prime "brute force primality test" b
bool is_prime(ll n) {
    if (n < 2)
        return false;
    if (n == 2)
        return true;
    if (n % 2 == 0)
        return false;
    for (ll i = 3; i * i <= n; i += 2)
        if (n % i == 0)
            return false;
    return true;
}
endsnippet

snippet code_miller_rabin_ptest "Miller & Rabin probabilistic primality test" b
bool miller_rabin_ptest(unsigned ll n, int k = 3) {
    if (n < 2) return false;
    if (n == 2) return true;

    while (k--) {
        int a = rand() % (n - 2) + 2;
        int p = n - 1;
        int r = 1;

        while (p) {
            if (p & 1)
                r = r * a % n;
            a = a * a % n;
            p >>= 1;
        }

        if (r != 1) return false;
    }

    prime_memo[n] = 1;
    return true; // probably
}
endsnippet

snippet code_primefact
map<ll, ll>
get_prime_factors(ll n) {
    map<ll, ll> result;
    int r = 0;

    while (n % 2 == 0) {
        r++;
        n = n / 2;
    }

    if (r > 0)
        result[2] = r;

    int sqn = sqrt(n);
    for (int i = 3; i <= sqn; i += 2) {
        r = 0;
        while (n % i == 0) {
            r++;
            n = n / i;
        }
        if (r > 0)
            result[i] = r;
    }

    if (n > 2)
        result[n] = 1;

    return result;
}
endsnippet

